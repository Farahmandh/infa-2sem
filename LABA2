A

#include <iostream>

int main() {
int a, b;
std::cin >> a >> b;
while( a != 0 && b != 0){
if( a > b)
a = a % b;
else
b = b % a;

}
std::cout << a + b;
}

B

#include <iostream>

int main() {
int n;
std::cin >> n;
if( (n % 4 != 0) || ((n % 100 == 0) && (n % 400 != 0))) 
std::cout << "no";
else std::cout << "yes";
}

C

#include <iostream>

int main() {
int n=-1,a;
do{
std::cin>>a;
if(a%2==0) n++;
}
while(a);
std::cout<<n;

}

D

#include <iostream>

int main() {
int n, a=1;
std::cin >> n;
while( a < n)
{
a*=2;
}
if(a == n) std::cout << "yes";
else std::cout << "no";

}

E

#include <iostream>

int main() {
int n, max = 0;
std::cin >> n;
while( n != 0){
std::cin >> n;
if( n > max && n % 2 == 0){ max = n;}
}
std::cout << max;
}

F

#include <iostream>

int main() {
int n, k = 0; 
std::cin >> n;
int max = n;
while( n != 0){
std::cin >> n;
if( n > max ){ max = n; k = 0;}
if( n == max ) {k++;}
}

std::cout << k;
}
